menu "FSL Sdmmc configuration"

menu "Host configuration"
    config FSL_SDMMC_USE_FSDIF
        bool "FSdif"
        select ENABLE_FSDIF

    config FSL_SDMMC_USE_FSDMMC
        bool "FSdmmc"
        select ENABLE_FSDMMC
endmenu

menu "Protocol configuration"
    config FSL_SDMMC_ENABLE_SD
        bool "SD"
        default y

    config FSL_SDMMC_ENABLE_MMC
        bool "eMMC"
        default y if FSL_SDMMC_USE_FSDIF

    config FSL_SDMMC_ENABLE_SDIO
        bool "SDIO"
        default n    

    config FSL_SDMMC_ENABLE_SD_SPI
        bool "SD SPI"
        default n    

endmenu

endmenu